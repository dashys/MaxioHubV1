   local f = s:Tab("LocalPlayer")
   f:Label ("LocalPlayer")


   f:Button ("Respawn" , function()
   local char = game.Players.LocalPlayer.Character
   if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid"):ChangeState(15) end
   char:ClearAllChildren()
   local newChar = Instance.new("Model")
   newChar.Parent = workspace
   game.Players.LocalPlayer.Character = newChar
   wait()
   game.Players.LocalPlayer.Character = char
   newChar:Destroy()
   end)

   f:Button ("Sit" , function()
   game.Players.LocalPlayer.Character.Humanoid.Sit = true
   end)

   f:Toggle ("Fly" , function(ezez)
   print ("Feature Not Done Yet!")
   end)



   f:Toggle ("Noclip" , function (FF)

   noclip = FF
   game:GetService('RunService').Stepped:connect(function()
   if noclip == true then
      game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
   end
   end)
   plr = game.Players.LocalPlayer
   mouse = plr:GetMouse()
   mouse.KeyDown:connect(function(key)
   if FF == false then
      game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
   end
   end)
   end)


   f:Slider("Walkspeed",0,400,70,function(t)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (t)
   end)

   f:Slider("JumpPower",0,400,70,function(p)
   game.Players.LocalPlayer.Character.Humanoid.JumpPower = (p)
   end)

   f:Slider("F0V",0,120,70,function(K)
